  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ajmas/sync-gdrive.git"
  },
  "author": "Andre John Mas",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^9.1.0",
    "googleapis": "^52.1.0",
    "mime": "^2.5.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/node": "^14.17.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^4.3.4",
    "dotenv": "^8.6.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.23.4",
    "esm": "^3.2.25",
    "mocha": "^7.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  }
